{
  "name": "relay-github-app",
  "homepage": "https://tar-aldev.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "build": "yarn run relay && react-scripts build",
    "relay": "yarn run relay-compiler --schema schema.graphql --src ./src/ --watchman false $@",
    "relay:watch": "yarn run relay-compiler --schema schema.graphql --src ./src/ --language typescript --watchman --watch $@",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "bootstrap": "4.6.0",
    "farce": "^0.4.5",
    "found": "^0.6.0",
    "found-relay": "^0.9.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-relay": "^11.0.2",
    "react-scripts": "4.0.3",
    "relay-runtime": "^11.0.2",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/react-relay": "^11.0.2",
    "@types/relay-runtime": "^11.0.1",
    "babel-plugin-relay": "^11.0.2",
    "gh-pages": "^3.2.3",
    "graphql": "^15.5.1",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
